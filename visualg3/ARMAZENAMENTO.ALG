algoritmo "Armazenamento"
var
   a: real
   b: real
   n: real
inicio
   escreval ("Digite o valor da variável A: ")
   leia (n)
   se (n>=0) entao
   a <- n
      escreval ("O número:",a," é variável de A")
   senao
   b <- n
      escreval ("O número:",b," é variável de B")
   fimse
   fimalgoritmo